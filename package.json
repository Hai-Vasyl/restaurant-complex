  "scripts": {
    "====== BUILD ======": "",
    "build:dev:client": "cross-env NODE_ENV=development webpack --mode development --config webpack.client.js",
    "build:dev:server": "cross-env NODE_ENV=development webpack --mode development --config webpack.server.js",
    "build:prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.prod.js",
    "====== WATCH ======": "",
    "watch:server": "cross-env NODE_ENV=development webpack --mode development --config webpack.server.js --watch",
    "====== START ======": "",
    "start:server": "nodemon dist/server/server.js",
    "start:dev:client": "cross-env NODE_ENV=development webpack serve --config webpack.client.js --mode development",
    "start:dev:server": "concurrently \"yarn watch:server\" \"yarn start:server\"",
    "start": "concurrently \"yarn start:dev:server\" \"yarn start:dev:client\""
  },
  "devDependencies": {
